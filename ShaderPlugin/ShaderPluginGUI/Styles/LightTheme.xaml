<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ShaderPluginGUI">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Theme.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Color x:Key="BorderNormal">#FFb5b5b5</Color>
    <Color x:Key="BorderPressed">#FF939393</Color>
    <Color x:Key="BackgroundNormal">#FFd6d6d6</Color>
    <Color x:Key="BackgroundSelected">#FF363636</Color>
    <Color x:Key="GridSplitLight">#FF636363</Color>
    <Color x:Key="DisabledForeground" R="33" G="33" B="33" A="170"/>
    <Color x:Key="UnselectedForeground" R="33" G="33" B="33" A="255"/>

    <Color x:Key="PSButtonBorderNormal">#FFe1e1e1</Color>
    <Color x:Key="PSButtonCancelBorderNormal">#FF6b6b6b</Color>
    <Color x:Key="PSButtonBorderFocused">#FF4971CD</Color>
    <Color x:Key="PSButtonBorderPressed">#FF3c5ba2</Color>


    <Color x:Key="PSButtonBackgroundPressed">#FFe1e1e1</Color>
    <Color x:Key="PSButtonCancelBackgroundPressed">#FF7d7d7d</Color>
    <Color x:Key="PSButtonBackgroundHover">#FF585858</Color>

    <SolidColorBrush x:Key="PSButtonForegroundBrush" Color="#FF000000"/>
    <SolidColorBrush x:Key="TextEditorBackgroundBrush" Color="#FFFFFF"/>

    <GradientStopCollection x:Key="PSScrollBarBackgroundStops">
        <GradientStop Color="#FF333333" Offset="0"/>
        <GradientStop Color="#FF434343" Offset="0.3125"/>
        <GradientStop Color="#FF3B3B3B" Offset="0.99"/>
    </GradientStopCollection>

    <GradientStopCollection x:Key="PSScrollBarThumbStops">
        <GradientStop Color="#FF9a9a9a" Offset="1"/>
        <GradientStop Color="#FF8b8b8b" Offset="0.99"/>
        <GradientStop Color="#FF6b6b6b" Offset="0"/>
    </GradientStopCollection>


    <GradientStopCollection x:Key="PSScrollBarLineButtonStops">
        <GradientStop Color="#5e5e5e" Offset="1"/>
        <GradientStop Color="#5e5e5e" Offset="0.95"/>
        <GradientStop Color="#4e4e4e" Offset="0.94"/>
        <GradientStop Color="#4a4a4a" Offset="0"/>
    </GradientStopCollection>

    <LinearGradientBrush  x:Key="PSTabItemNormalBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFb5b5b5"/>
        <GradientStop Color="#FFafafaf" Offset="1"/>
        <GradientStop Color="#FFb5b5b5" Offset="0.03"/>
        <GradientStop Color="#FFafafaf" Offset="0.032"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PSTabItemHoverBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFadadad"/>
        <GradientStop Color="#FFc5c5c5" Offset="1"/>
        <GradientStop Color="#FFadadad" Offset="0.03"/>
        <GradientStop Color="#FFc5c5c5" Offset="0.032"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PSTabItemSelectedBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFe8e8e8"/>
        <GradientStop Color="#FFd6d6d6" Offset="1"/>
        <GradientStop Color="#FFe8e8e8" Offset="0.03"/>
        <GradientStop Color="#FFd6d6d6" Offset="0.032"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PSRoundButtonHover" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF565656" Offset="1"/>
        <GradientStop Color="#FF717171" Offset="0.047"/>
        <GradientStop Color="#FF939393"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PSRoundButtonSelected" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF565656" Offset="1"/>
        <GradientStop Color="#FF717171" Offset="0.047"/>
        <GradientStop Color="#FF939393"/>
    </LinearGradientBrush>

    <LinearGradientBrush  x:Key="GridSplitBrush" EndPoint="0,0" StartPoint="1,0">
        <GradientStop Color="{StaticResource BorderNormal}" Offset="0.499"/>
        <GradientStop Color="{StaticResource GridSplitLight}" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="PSScrollBorderBrush" Color="#FF3a3a3a"/>
    <SolidColorBrush x:Key="TransparentBrush">#00000000</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundBrush">#FF212121</SolidColorBrush>

    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="{StaticResource DisabledForeground}" />
    <SolidColorBrush x:Key="UnselectedForegroundBrush" Color="{StaticResource UnselectedForeground}" />
    <SolidColorBrush x:Key="BorderBrushNormal" Color="{StaticResource BorderNormal}"/>
    <SolidColorBrush x:Key="BorderBrushPressed" Color="{StaticResource BorderPressed}"/>
    <SolidColorBrush x:Key="BackgroundBrushNormal" Color="{StaticResource BackgroundNormal}"/>
    <SolidColorBrush x:Key="BackgroundBrushSelected" Color="{StaticResource BackgroundSelected}"/>

    <Color x:Key="ToolBarGradientLine_MouseOver">#939393</Color>
    <Color x:Key="ToolBarGradientUp_MouseOver">#707070</Color>
    <Color x:Key="ToolBarGradientDown_MouseOver">#585858</Color>

    <Color x:Key="ToolBarGradientLine_Focused">#838383</Color>
    <Color x:Key="ToolBarGradientUp_Focused">#656565</Color>
    <Color x:Key="ToolBarGradientDown_Focused">#565656</Color>

</ResourceDictionary>